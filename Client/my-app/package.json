    "jsonwebtoken": "^8.3.0",
    "material-ui-upload": "^1.2.1",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-image-lightbox": "^5.0.0",
    "react-light-notifications": "^1.0.6",
    "react-notifications": "^1.4.3",
    "react-redux": "^5.0.7",
    "react-responsive-carousel": "^3.1.41",
    "react-router-dom": "^4.3.1",
    "react-s-alert": "^1.4.1",
